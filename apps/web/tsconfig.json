{
  "extends": "@sasvoth/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
       "@maci-protocol/contracts": ["../../packages/contracts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts",
     "../../packages/contracts/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
